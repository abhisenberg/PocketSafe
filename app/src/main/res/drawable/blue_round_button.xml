            <!--<stroke android:color="#c10000" android:width="5dp" />-->
            <!--<solid android:color="#c70000"/>-->
            <stroke android:color="#06426f" android:width="5dp" />
            <solid android:color="#0c68ac"/>
        </shape>
    </item>

